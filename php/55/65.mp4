function channellistExtend(extend){if(!$('.show-more').hasClass('hidden')){$('.show-more').addClass('hidden');var count=$('.channel-list .text-center').children('.channel-card').length;$.post(window.location.href+'extend/',{'csrfmiddlewaretoken':Cookies.get('csrftoken'),'offset':count},function(result){if(result.success){$('.channel-list').append(result.html);spaAttachEvents();$('[data-toggle="tooltip"]').tooltip({container:'body',trigger:'hover'});if($('.channel-list .text-center').children('.channel-card').length==count+extend)$('.show-more').removeClass('hidden');}});}}