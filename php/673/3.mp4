body.gdpr-modal-on{overflow:hidden}#gdpr-modal{position:fixed;top:0;right:0;left:0;bottom:0;padding:0;margin:0;width:100%;height:100%;z-index:99999999900000}#gdpr-modal *{font-family:Helvetica, Arial, sans-serif;font-size:16px}#gdpr-consent-overlay{width:100%;height:100%;opacity:0.9;background-color:white}#gdpr-consent-form{position:fixed;top:0;right:0;left:0;bottom:0;background-color:white;border:3px solid #3367d6;margin:auto;width:85%;height:90%;max-width:900px;max-height:900px;overflow:hidden}#gdpr-consent-wrapper{position:relative;height:100%}#gdpr-consent-header{margin:0 12px;padding:0}#gdpr-consent-header h5{color:#3367d6;font-size:16px;font-weight:600;text-align:left;line-height:1.2em;margin:0;padding:10px 0 6px 0}#gdpr-consent-header h5 svg{display:none;height:28px;vertical-align:middle;margin-bottom:5px;fill:#3367d6}#gdpr-consent-header span{color:black;display:inline-block;font-size:14px;line-height:1.36em;margin-bottom:12px;display:block;height:98px;overflow:hidden;overflow-y:scroll;background:linear-gradient(#fff 80%, #DfDfDf)}#gdpr-consent-header span i{font-size:14px;line-height:1.36em}#gdpr-consent-header span a{font-size:14px;line-height:1.36em;color:#333}#gdpr-consent-content{height:auto;overflow:hidden;overflow-y:scroll;border:1px solid #ccc;padding:0;margin:0 10px}#gdpr-consent-content h6{font-weight:600;margin:14px 0 5px 12px;line-height:1.3em}#gdpr-consent-content span{display:inline-block;margin:0 12px}#gdpr-consent-content > span{font-size:14px}#gdpr-consent-footer{bottom:0;height:60px;margin:0;position:absolute;width:100%;background-color:white}#gdpr-modal ul{display:block;list-style:none;padding:0;margin:8px 0 10px 8px}#gdpr-modal ul li{margin:5px 0}#gdpr-modal ul .privacy-link{font-size:14px;text-decoration:none}#gdpr-modal ul li span{margin:0 6px}#gdpr-modal ul li input{transform:scale(1.12);width:16px;margin:4px 6px}#gdpr-consent-footer a{padding:0;margin:20px 12px;display:inline-block;font-size:16px}#gdpr-modal button{float:right;height:30px;line-height:28px;padding:0 12px;margin:14px 10px 0 0;background:#0085ba;border-color:#0073aa;font-size:14px;color:#fff;text-decoration:none;cursor:pointer;border-width:1px;border-style:solid;border-radius:5px;white-space:nowrap}#gdpr-modal button.small{float:none;padding:0 14px 2px;margin:5px 20px 15px 0;background:#f7f7f7;border-color:#cccccc;font-size:14px;color:#000}#gdpr-button-fixed{position:fixed;bottom:0;right:0;z-index:99000099900;width:130px;height:20px;background-color:white;padding:6px 2px 5px 8px;border-top-left-radius:10px;border:1px solid #ccc;border-bottom:0;border-right:0;cursor:pointer;box-sizing:content-box}#gdpr-button-fixed > div{display:inline}#gdpr-button-fixed svg{height:100%;display:inline-block;width:20px;float:left;fill:#3367d6}#gdpr-button-fixed span{float:left;font-family:Helvetica, Arial;font-size:14px;text-decoration:none;line-height:20px;margin:0 0 0 4px;color:#3367d6}.service-label{display:none}@media (min-width:801px){#gdpr-consent-header h5{font-size:28px;margin:0 10px}#gdpr-consent-header h5 svg{display:inline-block}#gdpr-consent-header span{display:inline-block;overflow:auto;height:auto;margin:0 10px;background:none}#gdpr-consent-content > span{display:inline-block;font-size:16px}#gdpr-modal ul li{margin:8px 0}#gdpr-modal button{padding:0 18px 2px;margin:10px 20px}#gdpr-consent-content{margin:0 20px;margin-top:8px}#gdpr-consent-footer a{margin:20px}}