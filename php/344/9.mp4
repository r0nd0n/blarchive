/*
-----------------------------------
              GRID
-----------------------------------
*/
/*
    A mixin to generate span classes for use in FP1's grid.
*/
/*
  A mixin to allow assigning the appropriate width for a given span value, depending
  on the current screen width. Useful for situations where, for example, an element
  might be span9 on desktop and span12 on tablet and mobile, so you can do something like:

  .element
  {
    .Span(9);

    @media(max-width: @maxWidthTablet)
    {
      .Span(12);
    }
  }
*/
/*
    Medium Screens (width >= 980px and < 1200px)
*/
@media (min-width: 980px) {
  .wrapper {
    margin: 0 auto;
    position: relative;
    width: 940px;
  }
  .wrapper .row {
    margin-left: -20px;
    *zoom: 1;
  }
  .wrapper .row:before,
  .wrapper .row:after {
    display: table;
    content: "";
  }
  .wrapper .row:after {
    clear: both;
  }
  .wrapper [class*="span"] {
    float: left;
    margin-left: 20px;
  }
  .wrapper .span12 {
    width: 940px;
  }
  .wrapper .span11 {
    width: 860px;
  }
  .wrapper .span10 {
    width: 780px;
  }
  .wrapper .span9 {
    width: 700px;
  }
  .wrapper .span8 {
    width: 620px;
  }
  .wrapper .span7 {
    width: 540px;
  }
  .wrapper .span6 {
    width: 460px;
  }
  .wrapper .span5 {
    width: 380px;
  }
  .wrapper .span4 {
    width: 300px;
  }
  .wrapper .span3 {
    width: 220px;
  }
  .wrapper .span2 {
    width: 140px;
  }
  .wrapper .span1 {
    width: 60px;
  }
}
/*
    Tablets (width >= 768px and < 980px)
*/
@media (min-width: 768px) and (max-width: 979px) {
  .wrapper {
    margin: auto;
    width: 724px;
  }
  .row {
    margin-left: -20px;
    *zoom: 1;
  }
  .row:before,
  .row:after {
    display: table;
    content: "";
  }
  .row:after {
    clear: both;
  }
  [class*="span"] {
    float: left;
    margin-left: 20px;
  }
  .span12 {
    width: 724px;
  }
  .span11 {
    width: 662px;
  }
  .span10 {
    width: 600px;
  }
  .span9 {
    width: 538px;
  }
  .span8 {
    width: 476px;
  }
  .span7 {
    width: 414px;
  }
  .span6 {
    width: 352px;
  }
  .span5 {
    width: 290px;
  }
  .span4 {
    width: 228px;
  }
  .span3 {
    width: 166px;
  }
  .span2 {
    width: 104px;
  }
  .span1 {
    width: 42px;
  }
}
/*
  Mobile (width < 768px)
*/
@media (max-width: 767px) {
  .wrapper {
    padding: 0 3%;
    width: 100%;
  }
  .content.wrapper {
