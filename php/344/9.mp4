.article-content {
  margin-top: 30px;
  overflow-x: hidden;
  overflow-y: hidden;
  font-family: 'Raleway', sans-serif;
  /*
      Medium Desktop Screens (width >= 980px)
  */
  /*
      Tablets (width >= 768px and < 980px)
  */
  /*
      Mobile (width < 768px)
  */
  /*
    PAGINATION BUTTONS
  */
}
